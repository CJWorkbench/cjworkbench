{
  "presets": [
    [
      "@babel/react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/env",
      {
        "targets": ">1%, not dead, not IE > 0"
      }
    ]
  ],
  "plugins": [
    "macros",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "plugins": [
        "dynamic-import-node"
      ]
    }
  }
}
