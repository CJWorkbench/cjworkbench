    "guid": "0.0.12",
    "gulp": "^3.8.11",
    "gulp-react": "^3.0.0",
    "http-server": "^0.7.3",
    "jsdom": "^4.1.0",
    "jsx-loader": "^0.12.2",
    "mocha": "^2.2.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.4",
    "should": "^5.2.0",
    "sorty": "^1.2.0",
    "style-loader": "^0.8.2",
    "stylus-loader": "^0.4.0",
    "webpack": "^1.8.4",
    "webpack-dev-middleware": "^1.0.11",
    "webpack-dev-server": "^1.6.6"
  },
  "dependencies": {
    "buffer-function": "^1.0.0",
    "clone": "^0.1.18",
